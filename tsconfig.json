{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}